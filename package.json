    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.13",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.16",
    "monk": "^6.0.6",
    "morgan": "~1.9.0",
    "validator": "^10.11.0"
